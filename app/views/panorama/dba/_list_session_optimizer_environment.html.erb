<%
column_options = []
column_options << {:caption=>'Name',          :data=>proc{|rec| rec.name },           :title=>'Name of parameter'}
column_options << {:caption=>'SQL Feature',   :data=>proc{|rec| rec.sql_feature },    :title=>'Associated SQL feature'} if get_db_version >= '11.2'
column_options << {:caption=>'Default',       :data=>proc{|rec| rec.isdefault },      :title=>'Indicates whether the parameter is set to the default value (YES) or not (NO)'}
column_options << {:caption=>'Value',         :data=>proc{|rec| rec.value },          :title=>'Value of the parameter for the session'}

%>
<%= gen_slickgrid(@env, column_options, {
                                :caption => "Optimizer environment for instance=#{@instance}, SID=#{@sid}",
                                :width=>:auto
                        }) %>
