<%
column_options =
[
  {:caption=>"SID",             :data=>proc{|rec| fn(rec.sid)},                     :title=>"SIDSession-ID" },
  {:caption=> "SN",             :data=>proc{|rec| formattedNumber(rec.serial_no)},   :title=>"Serialnumber der Session", :align=>"right"},
  {:caption=>"Wait Time",       :data=>proc{|rec| fn(rec.wait_time)},               :title=>"Wait Time",                :align=>"right"},
  {:caption=>"Seconds in Wait", :data=>proc{|rec| fn(rec.seconds_in_wait)},         :title=>"Seconds in Wait",          :align=>"right"},
  {:caption=>"State",           :data=>proc{|rec| fn(rec.state)},                   :title=>"Status" },
  {:caption=>"Name",            :data=>proc{|rec| fn(rec.name)},                    :title=>"Name des Objektes" },
]

%>

<%=
    gen_slickgrid(@waits, column_options, {
            :caption => "Waits wegen db file sequential read",
            :width=>:auto,
    })
%>
