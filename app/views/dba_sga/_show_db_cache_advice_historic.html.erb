<% @update_area = get_unique_area_id %>

<%= render_page_caption("Historic DB-cache advice from #{PanoramaConnection.adjust_table_name('DBA_Hist_DB_Cache_Advice')}") %>

<%= ajax_form(:controller=>:dba_sga, :action=>:list_db_cache_advice_historic, :update_area=>@update_area)  do %>
  <div class="flex-row-container">
    <%= include_start_end_timepicker %>
    <%= instance_tag(rac_only: true) %>
    <div class="flex-row-element">
      <%= submit_tag("Show", :title => "Show historic DB-cache advices") %>
    </div>
  </div>
<% end %>
<div id=<%= @update_area %> style="clear:both"></div>
