<%= render_page_caption("Latch-Statistics from #{PanoramaConnection.adjust_table_name('DBA_Hist_Latch')}") %>

<%= ajax_form(:controller => :dba_history, :action => :list_latch_statistics_historic, :update_area => :list_latch_statistics_historic_area)  do %>
  <div class="flex-row-container">
    <%= include_start_end_timepicker %>
    <%= instance_tag(rac_only: true) %>
    <div class="flex-row-element">
      <%= submit_tag("Show Latch-Info", :title => "Show latch info") %>
    </div>
  </div>
<% end %>
<div id="list_latch_statistics_historic_area" style="float:left; width:100%; ">
</div>

